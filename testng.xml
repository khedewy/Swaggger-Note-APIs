<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite">
    <parameter name="browser" value="chrome" />
    <test name="Regression Test in chrome">
        <classes>
            <class name="tests.NotesTest">
                <methods>
                    <include name="register" />
                    <include name="login" />
                    <include name="createNote"/>
                    <include name="getNoteCreatedById"/>
                    <include name="putNote"/>
                    <include name="patchNote"/>
                    <include name="deleteNote"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>